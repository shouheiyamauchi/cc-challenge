{
  "name": "cc",
  "version": "1.0.0",
  "main": "src/start.ts",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/start.ts",
    "build": "npm run tslint && npm run build-ts",
    "build-ts": "rm -rf dist && tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "prettier": "prettier src/**/*.ts",
    "prettier:fix": "npm run prettier -- --write"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run prettier:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run build"
    }
  },
  "dependencies": {
    "aws-sdk": "^2.496.0",
    "dotenv": "^8.0.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "log-update": "^3.2.0",
    "p-limit": "^2.2.0"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/fs-extra": "^8.0.0",
    "@types/glob": "^7.1.1",
    "@types/log-update": "^3.1.0",
    "@types/p-limit": "^2.2.0",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  }
}
